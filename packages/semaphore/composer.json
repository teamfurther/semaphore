{
  "name": "semaphore/semaphore",
  "description": "Semaphore is an extensible dashboard for viewing Prometheus time series, built with Laravel, Tailwind and VueJS.",
  "keywords": [
    "further",
    "furtherdigitalsolutions",
    "monitoring",
    "laravel",
    "prometheus",
    "semaphore",
    "server monitoring"
  ],
  "homepage": "https://github.com/teamfurther/semaphore",
  "license": "MIT",
  "authors": [
    {
      "name": "Further Digital Solutions",
      "email": "hello@gofurther.digital"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^6.0|^7.0|^8.0",
    "laravel/slack-notification-channel": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Semaphore\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Semaphore\\SemaphoreServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
