{
    "name": "teamfurther/semaphore",
    "description": "Semaphore is an extensible dashboard for viewing Prometheus time series.",
    "keywords": [
        "further",
        "furtherdigitalsolutions",
        "monitoring",
        "laravel",
        "prometheus",
        "semaphore",
        "server monitoring"
    ],
    "homepage": "https://github.com/teamfurther/semaphore",
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "Peter Ill√©s",
            "email": "peter.illes@gofurther.digital",
            "homepage": "https://gofurther.digital",
            "role": "Developer"
        },
        {
            "name": "Csongor Ur",
            "email": "csongor.ur@gofurther.digital",
            "homepage": "https://gofurther.digital",
            "role": "Developer"
        },
        {
            "name": "Norbert Zsombori",
            "email": "norbert.zsombori@gofurther.digital",
            "homepage": "https://gofurther.digital",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
